<p id="notice"><%= notice %></p>

<h1>Listing Staff Addesses</h1>

<table>
  <thead>
    <tr>
      <th>Postalcode</th>
      <th>Addessinfo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staff_addesses.each do |staff_addess| %>
      <tr>
        <td><%= staff_addess.postalCode %></td>
        <td><%= staff_addess.addessInfo %></td>
        <td><%= link_to 'Show', staff_addess %></td>
        <td><%= link_to 'Edit', edit_staff_addess_path(staff_addess) %></td>
        <td><%= link_to 'Destroy', staff_addess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Addess', new_staff_addess_path %>
